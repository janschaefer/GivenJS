{
  "name": "given-html-report",
  "version": "0.0.0",
  "description": "HTML Report generator for GivenJS",
  "main": "index.js",
  "bin": {
    "given-html-report": "./bin/given-html-report"
  },
  "files": [
    "app",
    "bin",
    "index.js"
  ],
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "standard": "^5.2.1"
  },
  "scripts": {
    "mvn": "rm -rf tmp && mkdir -p tmp && cd tmp && wget http://central.maven.org/maven2/com/tngtech/jgiven/jgiven-html5-report/0.8.0/jgiven-html5-report-0.8.0.jar",
    "preunzip": "npm run mvn",
    "unzip": "cd tmp && unzip jgiven-html5-report-0.8.0.jar com/tngtech/jgiven/report/html5/app.zip",
    "preappunzip": "npm run unzip",
    "appunzip": "rm -rf app && mkdir -p app && unzip tmp/com/tngtech/jgiven/report/html5/app.zip -d app",
    "standard": "standard index.js",
    "preinstall": "npm run appunzip"
  },
  "keywords": [
    "bdd",
    "testing",
    "tests",
    "GivenJS",
    "HTML",
    "report",
    "JGiven"
  ],
  "author": "Jan Sch√§fer",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.8.1",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.14"
  }
}
